.mainbox{
    padding:1rem 10px 10px;
    h1{
	   font-size: 16px;
	    font-weight: 500;
	    text-align: center;
	}
	p{
	    line-height: 1.8;
	}
	h2{
	    font-size: 16px;
	    margin-top:20px;
	    margin-bottom: 8px;
	    color: #000;
	}
	h3{
	    font-size: 14px;
	    line-height: 1.8;
	    margin-top:10px;
	    margin-bottom:5px;
	    text-indent: 20px;
	}
	ul,ol{
	    text-indent: 20px;
	    font-size: 12px;
	    line-height: 1.8
	}
	h3 + p{
	    text-indent: 20px;
	    font-size: 12px;
	    color: #666;
	}
	h2 + ul,table +ul {
	    text-indent: 20px;
	    font-size: 14px;
	    line-height: 2
	}
	p + ul,p + ol,label + ul,label + ol{
	    text-indent: 20px;
	    font-size: 12px;
	    line-height: 1.8
	}
	strong {
	    font-weight: bold;
	}
	.liru{
	    padding: 10px;
	}
	.liru label{
	    font-size: 16px;
	    line-height: 2;
	}
	.liru div{
	    font-size: 14px;
	    line-height: 2;
	}
	.liru div + ul{
	   text-indent: 20px;
	    font-size: 12px;
	    line-height: 1.8 
	}
}
