.header{position:fixed;z-index:100;width:100%;left:0;top:0;height:1rem;background:linear-gradient(0deg,#399f00,#26701a);box-shadow:0 1px 0 0 #e0e0e0;color:#fff}.header .header-left{position:absolute;left:0;top:0;width:1rem;height:1rem;text-align:center;font-size:.5rem;line-height:1rem;color:#fff}.header .header-center{font-size:.36rem;line-height:1rem;text-align:center}.header .header-right{position:absolute;right:0;top:0;width:1rem;height:1rem;line-height:1rem;font-size:.28rem;text-align:center;color:#fff}.header .iconfont{font-size:.5rem}