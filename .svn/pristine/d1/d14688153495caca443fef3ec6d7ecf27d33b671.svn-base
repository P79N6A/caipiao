.mainbox{padding:1rem 10px 10px}.mainbox h1{font-size:16px;font-weight:500;text-align:center}.mainbox p{line-height:1.8}.mainbox h2{font-size:16px;margin-top:20px;margin-bottom:8px;color:#000}.mainbox h3{font-size:14px;line-height:1.8;margin-top:10px;margin-bottom:5px;text-indent:20px}.mainbox ol,.mainbox ul{text-indent:20px;font-size:12px;line-height:1.8}.mainbox h3+p{text-indent:20px;font-size:12px;color:#666}.mainbox h2+ul,.mainbox table+ul{text-indent:20px;font-size:14px;line-height:2}.mainbox label+ol,.mainbox label+ul,.mainbox p+ol,.mainbox p+ul{text-indent:20px;font-size:12px;line-height:1.8}.mainbox strong{font-weight:700}.mainbox .liru{padding:10px}.mainbox .liru label{font-size:16px;line-height:2}.mainbox .liru div{font-size:14px;line-height:2}.mainbox .liru div+ul{text-indent:20px;font-size:12px;line-height:1.8}